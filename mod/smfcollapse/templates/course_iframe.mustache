<div class="accordion-smf" style="width: 900px; "> 
    
    <div class="accordion accordion-content" id="smf1-1">

        <div class="card z-depth-0 bordered accordion-content-borderer mb-2" data-toggle="collapse" data-target="#parent_collapse{{parent_collapse}}"
                          aria-expanded="true" aria-controls="collapse_one">
            <div class="card-smf card-header card-header-carrousel >
                <h5 class="mb-0">
                    <button class="btn btn-name" type="button" >
                        <img style="width:8%" src="data:image/png;base64,"/> Pr√≥ximos Eventos Webex del curso  
                        <i class="fa fa-angle-down fa-rotate-vertical"></i>
                    </button>
                </h5>
            </div>
        </div>
        <div class="collapse in" id="parent_collapse{{parent_collapse}}">
            <div class="card card-body" style="border:0px !important;">



                {{#iframes}}
                    {{#name_bool}}
                        <div class="card z-depth-0 bordered accordion-content-borderer mb-2" data-toggle="collapse" data-target="#collapse{{sectionid}}"
                                aria-expanded="true" aria-controls="collapse{{sectionid}}">
                            <div class="card-smf card-header card-header-carrousel {{#chat_bool}}chat_smf{{/chat_bool}}" id="heading{{sectionid}}">
                                <h5 class="mb-0">

                                    <button class="btn btn-name" type="button" >
                                        {{name}}
                                        <i class="fa fa-angle-down fa-rotate-vertical"></i>
                                    </button>
                                </h5>
                            </div>
                                {{#url_bool}}
                                    <div id="collapse{{sectionid}}" class="collapse in" aria-labelledby="heading{{sectionid}}" data-parent="#collapse{{sectionid}}">
                                        <div class="card-body">
                                            <iframe src="{{url}}" id="frm{{sectionid}}" frameborder="0" hspace="0" vspace="0" marginheight="0" marginwidth="0" width="100%" ></iframe>
                                        </div>
                                    </div>
                                {{/url_bool}}
                        </div>
                    {{/name_bool}}
                {{/iframes}}


            </div>
        </div>       
    </div>
</div>